      "    if (edge.x > 0.5) {\n"
      "      return 8.0;\n"
      "    } else {\n"
      "      return ramp (coordinate, offset);\n"
      "    }\n"
      "  } else if (size - 1.0 - coordinate < 2.0 * offset) {\n"
      "    if (edge.y > 0.5) {\n"
      "      return 8.0;\n"
      "    } else {\n"
