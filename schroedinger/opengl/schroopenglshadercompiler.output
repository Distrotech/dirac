      "uniform vec2 size;\n"
      "float ramp (float coordinate, float offset) {\n"
      "  if (offset == 1.0) {\n"
      "    if (coordinate == 0.0) {\n"
      "      return 3.0;\n"
      "    } else {\n"
      "      return 5.0;\n"
      "    }\n"
      "  } else {\n"
      "    return 1.0 + divide_s16 (6.0 * coordinate + offset - 1.0, 2.0 * offset - 1.0);\n"
      "  }\n"
